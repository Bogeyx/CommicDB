    "@angular/forms": "4.2.2",
    "@angular/http": "4.2.2",
    "@angular/platform-browser": "4.2.2",
    "@angular/platform-browser-dynamic": "4.2.2",
    "@angular/platform-server": "4.2.2",
    "@angular/router": "4.2.2",

    "@types/node": "7.0.31",
    "@types/filesaver": "0.0.30",
    "@types/jquery": "2.0.46",

    "angular-in-memory-web-api": "0.3.2",
