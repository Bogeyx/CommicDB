{
  "version": "1.1.0",
  "name": "asp.net",
  "private": true,
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "watch1" :"node-sass -w wwwroot/css/site.scss -o wwwroot/css/",
    "watch2":"tsc -w -p .",
    "both": "concurrently --kill-others \"tsc -w -p .\" \"node-sass -w wwwroot/css/site.scss -o wwwroot/css/\""
  },
